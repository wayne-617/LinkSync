{
  "manifest_version": 3,
  "name": "LinkSync",
  "version": "1.0.0",
  "description": "Receive content from your iOS device.",
  "permissions": [
    "storage",
    "notifications",
    "gcm"
  ],

  "background": {
    "service_worker": "background.js"
  },

  "action": {
    "default_popup": "popup/popup.html"
  }
}